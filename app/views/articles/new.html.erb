<p bgcolor="#E6E6FA"><h1 >Add new article post </h1></p>
<%= form_for @article do |f| %>
	  <p><%= f.label :name %>
		<%= f.text_field :name %><br>
	  </p>
	   
	    <p>
	    <%= f.label :review%><br>
		<%= f.text_area :review %>
	  </p>
	  <p>
      <%= f.submit "Add new article post" %>
	  </p>
	<% end %>